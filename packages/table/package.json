{
  "name": "@blitzar/table",
  "version": "1.0.6",
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "description": "Blitzar table component",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint ./src/**.*",
    "gen:types:vetur": "vue-int -i src -r -o dist/vetur",
    "gen:types:ts": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "npm run lint && rimraf dist && vite build && npm run gen:types:ts && npm run gen:types:vetur",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "@blitzar/components": "^1.0.6",
    "@blitzar/form": "^1.0.6",
    "@blitzar/types": "^1.0.6",
    "is-what": "^3.14.1",
    "merge-anything": "^4.0.2",
    "path-to-prop": "^1.0.0"
  },
  "license": "MIT",
  "keywords": [
    "blitzar",
    "blitz-table",
    "data-tables",
    "data-table",
    "quasar-easy-tables",
    "quasar-easy-table",
    "easy-tables",
    "easy-table",
    "vue-table",
    "table",
    "table-component",
    "table-builder",
    "table-generator",
    "schema-table",
    "vue"
  ],
  "homepage": "https://github.com/cycraft/blitzar#readme",
  "bugs": {
    "url": "https://github.com/cycraft/blitzar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycraft/blitzar.git"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "gitHead": "a40cbe1a9bbc4727d4e7e6f833808f8c578762de"
}
