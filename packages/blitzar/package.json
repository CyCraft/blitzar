{
  "name": "blitzar",
  "version": "1.1.8",
  "type": "module",
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "description": "Generate Vue Forms and Data-tables fast with a simple JSON-like syntax âš¡",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "gen:styles": "cpy ../components/dist/style.css ./dist",
    "gen:types:vetur": "cpy ../components/dist/vetur/* ./dist/vetur",
    "gen:types:ts": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "rimraf dist && vite build && npm run gen:types:ts && npm run gen:styles && npm run gen:types:vetur",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "vue": "^3.2.29"
  },
  "dependencies": {
    "@blitzar/components": "^1.1.8",
    "@blitzar/types": "^1.1.8",
    "@blitzar/utils": "^1.1.8"
  },
  "homepage": "https://github.com/cycraft/blitzar#readme",
  "keywords": [
    "vue-framework",
    "vue",
    "vuejs",
    "component-framework",
    "prototyping",
    "fast-prototyping",
    "data-table",
    "data-tables",
    "table-generator",
    "table-builder",
    "tables",
    "form-generator",
    "form-builder",
    "forms",
    "schema"
  ],
  "license": "MIT",
  "bugs": "https://github.com/cycraft/blitzar/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycraft/blitzar.git"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "gitHead": "a7bd71f13d7be1c59da800dc2d5cf6435c22623d"
}
